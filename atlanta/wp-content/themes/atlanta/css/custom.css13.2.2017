/* CSS Document */

/* *************************************************************
************************** Header and Banner ******************************
*************************************************************** */

#slider {
	background: url(../images/banner.jpg);
	background-repeat: no-repeat;
	background-position: center;
	background-size: cover;
}
header {
	position: absolute;
	top: 0;
	z-index: 9;
	width: 100%;
	margin: 0 auto;
}
#slider {
	position: relative;
}
.doughted_back {
	background: url(../images/dout_back.png);
	background-repeat: no-repeat;
	background-position: center;
	background-size: cover;
}
.slider_overlay {
	background: rgba(0,0,0,.66);
}
nav {
	width: 100%;
	height: auto;
	padding: 50px 0;
}
.nav_wrap {
	display: inline-block;
	float: right;
	position: relative;
}
nav .logo {
	float: left;
	padding-top: 12px;
}
nav .navigation {
	display: inline-block;
	vertical-align: top;
	padding-top: 10px;
}
nav .navigation ul li {
	display: inline-block;
	vertical-align: top;
	position: relative;
}
nav .navigation ul li a {
	font-family: 'Lato-Bold';
	font-size: 13px;
	color: #e2b59e;
	text-transform: uppercase;
	padding: 14px 12px;
	transition: all 0.3s ease-in-out;
	-moz-transition: all 0.3s ease-in-out;
	-ms-transition: all 0.3s ease-in-out;
	-o-transition: all 0.3s ease-in-out;
	-webkit-transition: all 0.3s ease-in-out;
	line-height: 45px;
}
nav .navigation ul li:hover a {
	color: #fff;
	background: #e2b59e;
}
.navigation > ul > li:hover ul.submenu {
	display: block;
}
nav .navigation ul li ul.submenu {
	display: none;
	background: #e2b59e;
	width: 300px;
	position: absolute;
	top: 44px;
}
ul.submenu li a i {
	margin-right: 10px;
	font-size: 17px;
}
nav .navigation ul li ul.submenu li {
	display: block;
}
ul.submenu li {
	border-bottom: 1px solid #e8ccbe;
}
ul.submenu li:last-child {
	border-bottom: none;
}
ul.submenu li:hover a {
	color: #000 !important;
}
ul.submenu li a {
	transition: all 0.5s ease-in-out;
	-moz-transition: all 0.5s ease-in-out;
	-ms-transition: all 0.5s ease-in-out;
	-o-transition: all 0.5s ease-in-out;
	-webkit-transition: all 0.5s ease-in-out;
	display: block;
	color: #fff !important;
	padding: 7px 0px 7px 20px!important;
	line-height: 25px !important;
	text-align: left !important;
}
.search_wrapper {
	margin-left: 20px;
	display: inline-block;
}
.search_wrapper ul li {
	display: inline-block;
}
.search_wrapper ul li a i {
	vertical-align: bottom;
	display: block;
	padding-top: 10px;
	text-align: center;
	color: #000;
	font-size: 17px;
}
.app_button {
	text-align: center;
	padding: 25% 0 5%;
}
.app_button a {
	color: #000000;
	font-size: 13px;
	font-family: 'Lato-Black';
	background: #e2b59e;
	border: 1px solid #e2b59e;
	padding: 20px;
	display: inline-block;
	transition: all 0.5s ease-in-out;
	-moz-transition: all 0.5s ease-in-out;
	-ms-transition: all 0.5s ease-in-out;
	-o-transition: all 0.5s ease-in-out;
	-webkit-transition: all 0.5s ease-in-out;
}
.app_button a:hover {
	border: 1px solid #e2b59e;
	background: transparent;
	color: #fff;
}
.slider_contact {
	width: 100%;
	height: auto;
	border-top: 1px solid #6b6b6b;
	padding-top: 30px;
}
.slider_contact_container {
	width: 33%;
	float: left;
	color: #fff;
	text-align: center;
	border-right: 1px solid #6b6b6b;
	padding-bottom: 15px;
}
.slider_contact_container h5 {
	font-family: 'Rye-Regular';
	color: #e2b59e;
	font-size: 16px;
	padding-bottom: 5px;
	font-weight: normal;
}
.slider_contact_container .phone {
	font-size: 23px;
	font-family: 'Lato-Bold';
	color: #fff;
}
.slider_contact_container .phone a{
	color:#fff !important;
}
.slider_contact_container .email {
	font-family: 'Rye-Regular';
	color: #e2b59e;
	font-size: 18px;
	text-decoration: underline;
}
.slider_contact_container .email a{
	color: #e2b59e !important;
}
.slider_contact_container ul li {
	font-size: 18px;
	font-family: 'Lato-Regular';
	line-height: 30px;
}
.req_comment {
	font-family: 'Rye-Regular' !important;
	color: #e2b59e;
	font-size: 18px;
}
.slider_contact_container:last-child {
	border-right: none;
}
/* ********************** The SPA ************************** */

.spa_wrapper {
	width: 100%;
	height: auto;
	background: #f6f6f6;
	background-image: url(../images/spa_back.png);
	background-repeat: no-repeat;
	background-position: center;
	background-size: cover;
	padding: 60px 0 40px;
}
.spa_wrapper h2 {
	position: relative;
}
.spa_sec_left {
	float: left;
	width: 65%;
	padding: 40px 0 0;
}
.spa_sec_right {
	float: right;
	padding: 40px 0 0;
}
.spa_wrapper .spa_sec_left p {
	font-size: 20px;
	/*font-family: 'Lato-Bold';*/
	font-family: 'Lato-Regular';
	color: #373737;
	text-align: left;
	line-height: 35px;
	margin: 20px 0;
}
/* ********************** Eye Candy ************************** */

.eye_candy {
	width: 100%;
	height: auto;
	background-image: url(../images/eye_candy.png);
	background-repeat: no-repeat;
	background-position: center;
	background-size: cover;
	padding: 60px 0;
}
.eye_candy h2 {
	position: relative;
}
.eye_candy h2::after {
	top: 67px;
	left: 36%;
}
.eye_candy_rigth {
	width: 51%;
	float: right;
}
.eye_candy_left {
	width: 44%;
	float: left;
}
.eye_candy_rigth p {
	font-size: 20px;
	font-family: 'Lato-Bold';
	color: #373737;
	text-align: left;
	line-height: 35px;
	margin: 50px 0 0;
	padding-right: 30px;
}
.eye_candy_left_img {
	width: 100%;
	height: auto;
	/*background-image: url(../images/eye_candy_img.png);*/
	background-repeat: no-repeat;
	background-position: -2px 137px;
}
.eye_candy_rigth p.last_para {
	margin: 10px 0;
}
/* ********************** service section ************************** */

.service {
	width: 100%;
	height: auto;
	background-image: url(../images/service_wrapper.jpg);
	background-repeat: no-repeat;
	background-position: center;
	background-size: cover;
}
.service_wrapper {
	width: 100%;
	height: auto;
	background: rgba(89,70,63,.6);
}
.service_wrapper2 {
	width: 100%;
	height: auto;
	background: rgba(65,55,53,.4);
	padding: 120px 0;
	position: relative;
}
.service_wrapper2:before {
	content: '';
	background: url(../images/section_top_hanger.png);
	width: 100%;
	height: 91px;
	position: absolute;
	top: 0;
	left: 0;
}
.service_wrapper2:after {
	content: '';
	background: url(../images/section_bottom_hanger.png);
	width: 100%;
	height: 91px;
	position: absolute;
	bottom: 0;
	left: 0;
}
.service h6 {
	font-family: AlexBrush-Regular;
	font-size: 27px;
	color: #f8f8f8;
	text-align: center;
	padding-bottom: 5px;
}
.service h2:after {
	content: '';
	background: url(../images/header_underline_white.png);
	width: 363px;
	height: 25px;
	position: absolute;
	top: 55px;
	left: 35%;
}
.service h2 {
	position: relative;
	font-size: 40px;
}
.service_slider_content h3 {
	color: #000;
	font-size: 18px;
	font-family: 'Lato-Black';
	text-align: center;
	padding-top: 15px;
	text-transform: uppercase;
	line-height: 25px;
}
.service_img_container {
	width: 60%;
	margin: 0 auto;
}
.service_slider_content {
	margin: 0 20px !important;
	background: #fff;
	padding: 20px 14px 30px !important;
	border: 3px solid #d6ae4c;
	min-height: 220px;
}
/* ********************** Press section ************************** */

.press h2 {
	position: relative;
}
.press {
	width: 100%;
	height: auto;
	background-image: url(../images/testi_back.png);
	background-repeat: no-repeat;
	background-position: center;
	background-size: cover;
	padding: 30px 0 40px;
}
.press h3 {
	text-align: left;
}
.press .service_slider_content {
	margin: 0 10px !important;
	background: transparent;
	padding: 0px !important;
	border: none;
	min-height: 220px;
}
.press .service_img_container {
	width: 100%;
}
/* ********************** Contact Us section ************************** */

.contact_us {
	width: 100%;
	height: auto;
	background-image: url(../images/contact_back.png);
	background-repeat: no-repeat;
	background-position: center;
	background-size: cover;
	position: relative;
}
.contact_overlay {
	background: rgba(0,0,0,.63)
}
.contact_wrapper {
	width: 100%;
	height: auto;
	background-image: url(../images/contact_wrapper.png);
	background-repeat: no-repeat;
	background-position: center;
	background-size: cover;
	padding: 11% 0;
}
.contact_us h2 {
	position: relative;
}
.contact_us h2::after {
	content: '';
	background: url(../images/header_underline_white.png);
	width: 363px;
	height: 25px;
	position: absolute;
	top: 55px;
	left: 35%;
}
.contact_us::before {
	content: '';
	background: url(../images/section_top_hanger.png);
	width: 100%;
	height: 91px;
	position: absolute;
	top: 0;
	left: 0;
}
.contact_us::after {
	content: '';
	background: url(../images/section_bottom_hanger.png);
	width: 100%;
	height: 91px;
	position: absolute;
	bottom: 0;
	left: 0;
}
.contact_us h6 {
	text-align: center;
	font-size: 16px;
	color: #fff;
	padding-top: 45px;
	font-family: 'Trocchi', serif;
	font-weight: normal;
}
.contact-div {
	padding-top: 45px;
}
.contact-left-box {
	float: left;
	width: 48%;
}
.contact-div h4 {
	color: #fff;
	font-family: "Rye-Regular" !important;
	font-size: 16px;
	font-weight: 300;
	position: relative;
	padding-bottom: 20px;
}
.contact-div h4:after {
	content: "";
	position: absolute;
	left: 0;
	bottom: 0;
	width: 70px;
	height: 2px;
	background: #e2b59e;
}
.contact-div .contact-left-box p {
	color: #fff;
	line-height: 28px;
	text-align: left;
	padding-top: 15px;
	font-family: 'Trocchi', serif;
	font-size: 15px;
}
.contact-left-box .addres-box {
	padding-top: 15px;
}
.contact-left-box .addres-box h5 {
	color: #e2b59e;
	font-family: "Rye-Regular" !important;
	font-size: 16px;
	font-weight: 300;
	position: relative;
	padding-bottom: 10px;
}
.contact-left-box .addres-box .addres-box-left {
	float: left;
	width: 48%;
}
.contact-left-box .addres-box .addres-box-right {
	float: right;
	width: 48%;
}
.contact-left-box .addres-box p {
	margin: 0;
	padding: 0;
	font-size: 18px;
	font-family: 'Lato-Regular';
	line-height: 32px;
}
.contact-left-box .addres-box a {
	color: #e2b59e;
	font-family: "Rye-Regular" !important;
	font-size: 18px;
	font-weight: 300;
	line-height: 32px;
	text-decoration: underline;
	font-weight: 500;
}
.contact-right-box {
	float: right;
	width: 48%;
}
