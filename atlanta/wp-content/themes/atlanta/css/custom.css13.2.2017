.submit-btn input {
	cursor: pointer;
	background: #e2b59e;
	padding: 15px 20px;
	font-family: 'Lato-Black';
	font-weight: 700;
	border: 1px #e2b59e solid;
	transition: all 0.5s ease-in-out;
	-moz-transition: all 0.5s ease-in-out;
	-ms-transition: all 0.5s ease-in-out;
	-o-transition: all 0.5s ease-in-out;
	-webkit-transition: all 0.5s ease-in-out;
	width:auto;
	line-height:10px;
	color: #000;
}
.submit-btn input:hover {
	background: none;
	border: 1px #e2b59e solid;
	color: #fff;
}
.gallery-cell {
	width: 100%;
}
.testimonial {
	text-align: center;
	max-width: 1000px;
	margin: 65px auto 30px auto;
	padding: 0 20px;
}
.testimonial-avatar {
	width: 100px;
	border-radius: 50%;
	border: 3px solid #e2b59e;
}
.testimonial-quote {
	display: block;
	font-size: 19px;
	font-weight: 300;
	padding: 10px 0;
	color: #5b5b5b;
	line-height: 30px;
}
.testimonial-author {
	display: block;
	color: #272727;
	font-size: 16px;
	font-family: 'Trocchi', serif;
}
.flickity-page-dots {
	bottom: 25px;
}
.flickity-page-dots .dot.is-selected {
	background: transparent !important;
	border: 2px solid #e2b59e !important;
}
/*30_01_2017 esha*/

.testimonial_sec {
	width: 100%;
	height: auto;
	padding: 30px 0 70px;
	background-image: url(../images/testi_back.png);
}
.testimonial_sec h2 {
	position: relative;
}
.testimonial_sec h2::after {
	left: 35%;
}
.testimonial_content {
	padding: 4%;
	box-shadow: 0px 0px 10px #dadada;
	background: #fff;
	margin-top: 30px;
}
.flickity-page-dots .dot {
	opacity: 1 !important;
	background: #e2b59e !important;
	margin: 4px !important;
}
/* ********************** Footer section ************************** */


.footer {
	width: 100%;
	height: auto;
	background-image: url(../images/footer_background.jpg);
	background-position: center;
	background-size: cover;
	background-repeat: no-repeat;
}
.footer_overlay {
	background: rgba(0,0,0,.78);
	padding: 70px 0 10px;
}
.map_img {
	background-image: url(../images/map_img.png);
	background-position: -32px 74px;
	background-size: inherit;
	background-repeat: no-repeat;
}
.footer_sec_content {
	width: 26%;
	float: left;
	vertical-align: top;
}
.footer_sec_content h5 {
	position: relative;
	font-family: 'Rye-Regular';
	font-size: 22px;
	color: #fff;
	font-weight: normal;
	text-transform: uppercase;
	text-align: left;
}
.footer_sec_content h5:after {
	content: '';
	background: url(../images/footer_underline.png);
	width: 40px;
	height: 3px;
	position: absolute;
	bottom: -15px;
	left: 0;
}
ul.footer_contact li ul li {
	display: inline-block;
	vertical-align: top;
}
ul.footer_contact li {
	padding-bottom: 10px;
}
.footer_contact, .latest_project, ul.quick_link, .sign_up {
	margin-top: 40px;
}
ul.footer_contact li ul li i {
	color: #bfbfbf;
	font-size: 18px;
	display: block;
	padding-top: 8px;
	padding-right: 10px;
}
.footer_contact p {
	font-size: 16px;
	color: #bfbfbf;
	text-align: left;
	margin: 2px 0;
}
.quick_sec {
	width: 20%;
}
ul.quick_link li {
	padding-bottom: 20px;
	padding-left: 20px;
}
ul.quick_link li a {
	color: #bebebe;
	font-size: 16px;
	transition: all 0.5s ease-in-out;
	-moz-transition: all 0.5s ease-in-out;
	-ms-transition: all 0.5s ease-in-out;
	-o-transition: all 0.5s ease-in-out;
	-webkit-transition: all 0.5s ease-in-out;
}
ul.quick_link li a:hover, .quick_link li:hover:before {
	color: #e2b59e;
}
.quick_link li {
	position: relative;
}
.quick_link li:before {
	content: "\f101";
	font-family: FontAwesome;
	font-style: normal;
	font-weight: normal;
	text-decoration: inherit;
	color: #bebebe;
	font-size: 18px;
	position: absolute;
	top: 0px;
	left: 0;
	transition: all 0.5s ease-in-out;
	-moz-transition: all 0.5s ease-in-out;
	-ms-transition: all 0.5s ease-in-out;
	-o-transition: all 0.5s ease-in-out;
	-webkit-transition: all 0.5s ease-in-out;
}
.latest_project li {
	display: inline-block;
	margin: 2px;
}
.sign_up .form_email {
	background: transparent;
}
.wrap_form_button {
	border: 1px solid #a8adb6;
}
.wrap_form_button ul li {
	display: inline-block;
	vertical-align: top;
}
li.email_input_sec {
	width: 80%;
}
.sign_up input {
	text-indent: 20px;
	border: none;
}
.sign_up input.form_button {
	background: transparent;
	border: none;
}
.email_input_sec_button {
	position: relative;
	width: 18%;
	cursor: pointer;
}
.wrap_form_button ul li i {
	position: absolute;
	color: #fff;
	top: 32%;
	right: 23%;
}
.sign_up .form_email::-webkit-input-placeholder { /* Chrome/Opera/Safari */
 color: #a8adb6;
 font-family: Lato-Regular;
}
.sign_up .form_email::-moz-placeholder { /* Firefox 19+ */
 color: #a8adb6;
 font-family: Lato-Regular;
}
.sign_up .form_email:-ms-input-placeholder { /* IE 10+ */
 color: #a8adb6;
 font-family: Lato-Regular;
}
.sign_up .form_email:-moz-placeholder { /* Firefox 18- */
 color: #a8adb6;
 font-family: Lato-Regular;
}
.bottom_footer {
	width: 100%;
	height: auto;
	background: #1d1d1d;
	padding: 25px 0;
}
.bottom_footer p {
	color: #aaaaaa;
	font-size: 14px;
	font-family: 'Lato-Regular';
	text-align: center;
	margin: 0;
}
.footer_link {
	text-align: right;
	display: inline-block;
	float: right;
	display:none;
}
.footer_link ul li {
	display: inline-block;
	color: #aaaaaa;
	font-size: 14px;
	font-family: 'Lato-Regular';
	margin: 0 5px;
}
.footer_link ul li a {
	color: #aaaaaa;
	transition: all 0.5s ease-in-out;
	-moz-transition: all 0.5s ease-in-out;
	-ms-transition: all 0.5s ease-in-out;
	-o-transition: all 0.5s ease-in-out;
	-webkit-transition: all 0.5s ease-in-out;
}
.footer_link ul li a:hover {
	color: #e2b59e;
}
/*----search-------*/
fieldset.search {
	border: none;
	width: 70px;
	float: right;
	padding: 0px;
}
.search > span {
	color: #000;
	font-size: 12px;
	padding-top: 7px;
	line-height: 32px;
	font-family: Avalon;
}
.search input, .search button {
	border: none;
	float: left;
}
.search input.box {
	color: #fff;
	font-size: 1em;
	width: 190px;
	height: 33px;
	padding: 1px 5px 3px;
	border: 1px solid #eeeeee;
}
.search input.box:focus {
	outline: none;
	border: 1px solid #000;
	color: #000;
}
.search a.btn {
	cursor: pointer;
	height: 40px;
	width: 40px;
	border-radius: 50%;
	transition: all 0.6s ease-in-out 0s;
	-moz-transition: all 0.6s ease-in-out 0s;
	-o-transition: all 0.6s ease-in-out 0s;
	-webkit-transition: all 0.6s ease-in-out 0s;
	float: right;
	color: #222;
	font-size: 20px;
	background: #fff;
	vertical-align: bottom;
	position: absolute;
	top: 8px;
	left: 8px;
}
/* search */

#search {
	position: fixed;
	top: 0px;
	left: 0px;
	width: 100%;
	height: 100%;
	background-color: rgba(0, 0, 0, 0.8);
	-webkit-transition: all 0.25s ease-in-out;
	-moz-transition: all 0.25s ease-in-out;
	-o-transition: all 0.25s ease-in-out;
	-ms-transition: all 0.25s ease-in-out;
	transition: all 0.25s ease-in-out;
	-webkit-transform: translate(0px, -100%) scale(0, 0);
	-moz-transform: translate(0px, -100%) scale(0, 0);
	-o-transform: translate(0px, -100%) scale(0, 0);
	-ms-transform: translate(0px, -100%) scale(0, 0);
	transform: translate(0px, -100%) scale(0, 0);
	opacity: 0;
	z-index: 99999;
